{
  "name": "zebras",
  "version": "0.0.13",
  "description": "Functional JS data analysis library built with Ramda",
  "main": "zebras.js",
  "unpkg": "zebras.js",
  "scripts": {
    "test": "nyc --reporter=html --reporter=text mocha --timeout=3000",
    "build:docs": "jsdoc --configure .jsdoc.json",
    "watch:docs": "onchange 'zebras.js' -- npm run build:docs",
    "watch:test": "onchange 'test/*.js' -- npm run test",
    "serve:docs": "live-server ./docs",
    "dev:docs": "npx run-p watch:docs serve:docs"
  },
  "keywords": [
    "data",
    "dataframe",
    "data-science",
    "data-analysis",
    "pandas",
    "csv",
    "r"
  ],
  "author": "Nick Levine",
  "license": "ISC",
  "repository": "https://github.com/nickslevine/zebras",
  "dependencies": {
    "cli-table3": "^0.5.1",
    "ramda": "^0.26.1"
  },
  "devDependencies": {
    "docdash": "^1.0.2",
    "eslint": "^5.12.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.14.0",
    "jsdoc": "^3.5.5",
    "live-server": "^1.2.1",
    "mocha": "^5.2.0",
    "npm-run-all": "^4.1.5",
    "nyc": "^13.1.0",
    "onchange": "^5.2.0",
    "prettier": "^1.15.3"
  },
  "prettier": {
    "semi": false,
    "tabWidth": 2,
    "useTabs": false,
    "singleQuote": false,
    "trailingComma": "es5",
    "bracketSpacing": true,
    "arrowParens": "avoid"
  },
  "eslintConfig": {
    "extends": "airbnb-base",
    "rules": {
      "semi": "off",
      "quotes": "double",
      "arrow-parens": "as-needed"
    }
  }
}
